{
  "background": false,
  "behavior": "application",
  "version": "0.1",
  "title": "{{ app }}",
  "name": "{{ appCapOne }}",
  "identifier": "com.koding.apps.{{ appLower }}",
  "path": "~/Applications/{{ appCap }}",
  "homepage": "{{ userLower }}.kd.io/{{ appLower }}",
  "repository": "git://github.com/{{ user }}/{{ appLower }}.kdapp.git",
  "description": "{{ appCap }} : a Koding application created with the blank template.",
  "category": "web-app",
  "source": {
    "blocks": {
      "app": {
        "files": [
          "./index.coffee"
        ]
      }
    },
    "stylesheets": [
      "./resources/style.css"
    ]
  },
  "options": {
    "type": "tab"
  },
  "icns": {
    "128": "./resources/icon.128.png"
  },
  "fileTypes": []
}